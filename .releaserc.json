{
  "branches": [
    "main",
    {
      "name": "dev",
      "prerelease": "beta"
    },
    {
      "name": "*",
      "prerelease": "alpha"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/exec", {
      "prepareCmd": "VERSION=${nextRelease.version} ./gradlew --no-daemon build"
    }],
    ["@semantic-release/github", {
      "assets": "build/libs/*.jar"
    }]
  ]
}
